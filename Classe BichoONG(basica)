public class BichoOng
{
   private String nomeOng, pessoaContato, tipoOng;
   private int telefone;
   //construtor parametrizado (problema em puxar os parametros da classe bicho para o construtor)
   public BichoOng(String nomeOng1, String pessoaContato1, String tipoOng1, int telefone1){
       setNomeOng(nomeOng);
       setPessoaContato(pessoaContato1);
       setTelefone(telefone1);
       setTipoOng(tipoOng1);
    }
   //todos os gets
   public String getNomeOng(){
       return nomeOng;
    }
   public String getPessoaContato(){
       return pessoaContato;
    }
   public int getTelefone(){
       return telefone;
    }
   public String getTipoOng(){
       return tipoOng;
    }
   //todos os sets
   public void setNomeOng(String nomeOng1){
       nomeOng=nomeOng1;
    }
   public void setPessoaContato(String pessoaContato1){
       pessoaContato=pessoaContato1;
    }
   public void setTelefone(int telefone1){
       telefone=telefone1;
    }
   public void setTipoOng(String tipoOng1){
       tipoOng=tipoOng1;
    }
   public String toString(){ 
       //FALTA BOTAR PARA RETORNAR AS INFORMAÇÕES DA CLASSE BICHO
       return "\n Nome da ONG: " + nomeOng + "\n Telefone: " + telefone + "\n Pessoa para contato: " + pessoaContato +
       "\n Tipo de ONG: ";
    } 
}
